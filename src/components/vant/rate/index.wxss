@import '../common/index.wxss';.van-rate {
  display: inline-flex;
  user-select: none;
}
.van-rate__item {
  position: relative;
  padding: 0 4rpx;
  padding: 0 var(--rate-horizontal-padding, 4rpx);
}
.van-rate__icon {
  display: block;
  height: 1em;
  font-size: 40rpx;
  font-size: var(--rate-icon-size, 40rpx);
}
.van-rate__icon--half {
  position: absolute;
  top: 0;
  width: 0.5em;
  overflow: hidden;
  left: 4rpx;
  left: var(--rate-horizontal-padding, 4rpx);
}
